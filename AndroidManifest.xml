<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="edu.wa.tacoma.team1.tcss450.stickmanwalking">
    <uses-permission android:name="android.permission."/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/stickman_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".chenwei.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER"  />
            </intent-filter>
        </activity>
        <activity android:name=".chenwei.ScoreActivity" >
        </activity>
        <activity android:name=".chenwei.GameoverActivity" >
        </activity>
        <activity android:name=".chenwei.GameActivity" >
        </activity>
        <activity android:name=".chenwei.authenticate.SignInActivity" >
        </activity>
        <activity android:name=".sally.GameActivityLink" >
        </activity>
    </application>


</manifest>